{
    "name": "coringawc/filament-input-loading",
    "description": "Input with loading spinner option",
    "keywords": [
        "laravel",
        "user",
        "cli",
        "resource",
        "ui",
        "filament",
        "widget",
        "fun"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/CoringaWc/filament-input-loading"
        }
    ],
    "autoload": {
        "psr-4": {
            "CoringaWc\\FilamentInputLoading\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Argemiro D Neto",
            "email": "nt_170@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "filament/forms": "^2.9",
        "spatie/laravel-package-tools": "^1.10"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "CoringaWc\\FilamentInputLoading\\FilamentInputLoadingProvider"
            ]
        }
    },
    "prefer-stable": true
}
